{
  "name": "final-project-server",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.js"
    }
  ],
  "env": {
    "CLIENT_BASE_URL": "https://<name>.<username>.now.sh",
    "MONGO_DB": "",
    "NODE_ENV": "production",
    "SECRET_KEY": ""
  }
}
